#################################################################
#    Object Properties
#################################################################

###  http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel
:authorEducationLevel rdf:type owl:ObjectProperty ;
                      rdfs:domain :PoeticWork ;
                      rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                   owl:hasValue <http://postdata.linhd.uned.es/kos/author-education-level>
                                 ] ;
                      rdfs:comment "Level of education of the creator of a text/song. This property has as Domain PoeticWork and not Person since the education level of a person changes during time. That is, this is the education level of the author in the moment of writing the poem."@en ;
                      rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                      rdfs:label "author Education Level"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#birthDate
:birthDate rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity> ;
           rdfs:comment "Date of birth"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "birthDate"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#birthPlace
:birthPlace rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :isFrom ;
            owl:inverseOf :birthPlaceOf ;
            rdfs:domain :Person ;
            rdfs:range :Place ;
            rdfs:comment "This property points to the Place which is the provenance of the current instance of Person"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "birth Place"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceCertainty
:birthPlaceCertainty rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :certainty ;
                     rdfs:domain :Person ;
                     rdfs:comment "Certainty of the date of birth of a Person"@en ;
                     rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                     rdfs:label "birth Place Certainty"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceOf
:birthPlaceOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Place ;
              rdfs:range :Person ;
              rdfs:comment "This property points to a Person who was born in the Place"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "birth Place Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#certainty
:certainty rdf:type owl:ObjectProperty ;
           rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                        owl:hasValue <http://postdata.linhd.uned.es/kos/certainty>
                      ] ;
           rdfs:comment "Level of confidence regarding the correspondent function"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "certainty"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#characterIn
:characterIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :hasCharacter ;
             rdfs:domain :Agent ;
             rdfs:range :Redaction ;
             rdfs:comment "This property points to the text in which the current instance of Person or Organisation is a character."@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "character In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#date
:date rdf:type owl:ObjectProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :Ensemble
                                  :Place
                                  :PoeticWork
                                  :Redaction
                                  <http://www.europeana.eu/schemas/edm/Event>
                                  <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role>
                                )
                  ] ;
      rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity> ;
      rdfs:comment "This property points to a date entity, which represents a particular point or period of time at which something happened or existed, or is expected to happen"@en ;
      rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
      rdfs:label "date"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#deathDate
:deathDate rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity> ;
           rdfs:comment "Date of death"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "deathDate"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#deathPlace
:deathPlace rdf:type owl:ObjectProperty ;
            owl:inverseOf :deathPlaceOf ;
            rdfs:domain :Person ;
            rdfs:range :Place ;
            rdfs:comment "This property points to the instance of Place where the Person died"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "death Place"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#deathPlaceOf
:deathPlaceOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Place ;
              rdfs:range :Person ;
              rdfs:comment "This property points to a Person who died in the current instance of Place"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "death Place Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#dedicatedTo
:dedicatedTo rdf:type owl:ObjectProperty ;
             owl:inverseOf :hasDedication ;
             rdfs:domain :Redaction ;
             rdfs:range :Person ;
             rdfs:comment "This property points to a Person to whom the current text was dedicated. This means that a Redaction includes a dedicated text to another person in an explicit way (frequently at the beginning of the text)."@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "dedicated To"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#depictedIn
:depictedIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Role ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :PoeticWork
                                       :Redaction
                                     )
                       ] ;
            rdfs:comment "This property points to the work (instance of PoeticWork) or a redaction which depicts the Role at hand in relation to its contents. When the Role at hand is related to the agents that participated in the transmission of the work (e.g. sponsor), this property points to the entity affected by the current function"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "depicted In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#editedIn
:editedIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Redaction ;
          rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
          rdfs:comment "This property points to the specific Location in a bibliographic item that contains an edition of the current instance of Redaction."@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "edited In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#editorOf
:editorOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :hasEditor ;
          rdfs:domain :Person ;
          rdfs:range :Redaction ;
          rdfs:comment "This property points to an instance of Redaction which was edited by the the current Person"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "editor Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#firstPartOf
:firstPartOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> ;
             owl:inverseOf :hasFirstPart ;
             rdfs:domain :TextUnit ;
             rdfs:range :Redaction ;
             rdfs:comment "This property points to the container (an instance of Redaction) of which the item at hand is its first part"@en ,
                          "first Part of"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" .


###  http://postdata.linhd.uned.es/ontology/postdata-core#floruit
:floruit rdf:type owl:ObjectProperty ;
         rdfs:domain :Person ;
         rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity> ;
         rdfs:comment "The period of activity of a person, used when a range cannot be established (e.g. “3rd century”)"@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
         rdfs:label "floruit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#gender
:gender rdf:type owl:ObjectProperty ;
        rdfs:domain :Person ;
        rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                     owl:hasValue <http://postdata.linhd.uned.es/kos/gender>
                   ] ;
        rdfs:comment "The gender of a Person"@en ;
        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
        rdfs:label "gender"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#genre
:genre rdf:type owl:ObjectProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :PoeticWork
                                   :Redaction
                                 )
                   ] ;
       rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                  [ rdf:type owl:Restriction ;
                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                    owl:hasValue <http://postdata.linhd.uned.es/kos/genre>
                  ] ;
       rdfs:comment "Type of art, literature or music characterized by a specific form, content and style"@en ;
       rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
       rdfs:label "genre"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:domain :Redaction ;
              rdfs:range :Agent ;
              rdfs:comment "This property points to a character depicted in the text of a Redaction, if any. The character is an instance of Agent (Organisation or Person)."@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "has Character"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasCreator
:hasCreator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Ensemble
                                        :PoeticWork
                                        :Redaction
                                      )
                        ] ;
            rdfs:range :CreatorRole ;
            rdfs:comment "This property points to the entity Creator that has created the current work under analysis"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "has Creator"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication
:hasDedication rdf:type owl:ObjectProperty ;
               rdfs:domain :Person ;
               rdfs:range :Redaction ;
               rdfs:comment "This property points to the text (an instance of Redaction) which was dedicated to the current Person."@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
               rdfs:label "has Dedication"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasEditor
:hasEditor rdf:type owl:ObjectProperty ;
           rdfs:domain :Redaction ;
           rdfs:range :Person ;
           rdfs:comment "This property points to the Person who has edited the current instance of Redaction"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "has Editor"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasFirstPart
:hasFirstPart rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
              rdfs:domain :OrderedTextUnitList ;
              rdfs:range :TextUnit ;
              rdfs:comment "This property points to the first text unit  which form the current element under analysis"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "has First Part"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isMemberOf ;
           rdfs:domain :Organisation ;
           rdfs:range :Person ;
           rdfs:comment "This property points to a Person that belongs to the Organisation"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "has Member"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasOrderedTextUnitList
:hasOrderedTextUnitList rdf:type owl:ObjectProperty ;
                        rdfs:domain :Redaction ;
                        rdfs:range :OrderedTextUnitList ;
                        rdfs:comment "List of text units that form the Redaction"@en ;
                        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                        rdfs:label "has Ordered Text Unit List"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasProvenance
:hasProvenance rdf:type owl:ObjectProperty ;
               owl:inverseOf :isProvenanceFor ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Ensemble
                                           :PoeticWork
                                         )
                           ] ;
               rdfs:range :Place ;
               rdfs:comment "This property points to the Place which is the provenance of the current instance of Ensemble or Poetic Work"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
               rdfs:label "has Provenance"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasSynthesis
:hasSynthesis rdf:type owl:ObjectProperty ;
              owl:inverseOf :isSynthesisOf ;
              rdfs:domain :Redaction ;
              rdfs:range :Redaction ;
              rdfs:comment "This property points to one of the texts (an instance of Redaction) which is an analytical version of the text at hand (the current Redaction)"@en ,
                           "has Synthesis"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasTextUnit
:hasTextUnit rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/ontology/olo/core#slot> ;
             rdfs:domain :OrderedTextUnitList ;
             rdfs:range :TextUnit ;
             rdfs:comment "A text unit in an ordered list."@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "hasTextUnit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#hasTranslation
:hasTranslation rdf:type owl:ObjectProperty ;
                owl:inverseOf :isTranslationOf ;
                rdfs:domain :Redaction ;
                rdfs:range :Redaction ,
                           <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
                rdfs:comment "This property points to another work (an instance of Redaction) which is a translation of this one"@en ,
                             "This property points to the specific location in a bibliographic item (an instance of Location) which contains a translation of the text at hand"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "has Translation"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#involves
:involves rdf:type owl:ObjectProperty ;
          owl:inverseOf :isInvolvedIn ;
          rdfs:domain :Event ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Agent
                                     :Place
                                   )
                     ] ;
          rdfs:comment "This property points to an Organisation, Person or Place which are related to the current instance of Event"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "involves"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isFrom
:isFrom rdf:type owl:ObjectProperty ;
        rdfs:range :Place ;
        rdfs:comment "This property points to the Place which is the provenance of the item"@en ;
        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
        rdfs:label "is From"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isIndexedIn
:isIndexedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Redaction ;
             rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
             rdfs:comment "This property points to the specific location in the text of the bibliographic item (an instance of Location) in which the current text (Redaction) is indexed."@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "is Indexed In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isInvolvedIn
:isInvolvedIn rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Agent
                                          :Place
                                        )
                          ] ;
              rdfs:range :Event ;
              rdfs:comment "This property points to the Event in which the current instance of Organisation, Person or Place is referenced"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "is Involved In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Person ;
            rdfs:range :Organisation ;
            rdfs:comment "This property points to the instance of Organisation to which the Person at hand belongs"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "is Member Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isMentionedIn
:isMentionedIn rdf:type owl:ObjectProperty ;
               owl:inverseOf :mentions ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Event
                                           :Place
                                           <http://xmlns.com/foaf/0.1/Organization>
                                           <http://xmlns.com/foaf/0.1/Person>
                                         )
                           ] ;
               rdfs:range :Redaction ;
               rdfs:comment "This  property points to the text (an instance of Redaction) that records the current item (Event, Place, Organization or Person) under analysis"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
               rdfs:label "is Mentioned In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isProvenanceFor
:isProvenanceFor rdf:type owl:ObjectProperty ;
                 rdfs:domain :Place ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( :Ensemble
                                            :PoeticWork
                                          )
                            ] ;
                 rdfs:comment "This property points to the Entity whose provenance corresponds to the current instance of Place"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                 rdfs:label "is Provenance For"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isRealisedThrough
:isRealisedThrough rdf:type owl:ObjectProperty ;
                   owl:inverseOf :realises ;
                   rdfs:domain :PoeticWork ;
                   rdfs:range :Redaction ;
                   rdfs:comment "This property points to a text (an instance of Redaction) which is one of the materialisations of the work under analysis"@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                   rdfs:label "is Realised Through"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isRecordedBy
:isRecordedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :Event ;
              rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
              rdfs:comment "This property points to the specific location in the text of the bibliographic item  (an instance of Location) that registers the current event"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "is Recorded By"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isReferencedIn
:isReferencedIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
                rdfs:comment "This property points to the specific location in a bibliographic item (an instance of Location) whose contents are related to the current item under analysis (usually, its specific bibliography)"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "is Referenced In"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isSynthesisOf
:isSynthesisOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Redaction ;
               rdfs:range :Redaction ;
               rdfs:comment "This property points to another text (an instance of Redaction) of which this version (Redaction) is a synthesis"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
               rdfs:label "is Synthesis Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isTranslationOf
:isTranslationOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Redaction ;
                 rdfs:range :Redaction ;
                 rdfs:comment "This property points to another version (an instance of Redaction) of which this one is a translation"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                 rdfs:label "is Translation Of"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#literaryPeriod
:literaryPeriod rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                             owl:hasValue <http://postdata.linhd.uned.es/kos/literary-period>
                           ] ;
                rdfs:comment """Label of a time frame for grouping certain works. Cf. \"school\".
The values of this property are defined in a controlled vocabulary about literary periods."""@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "literary Period"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#mentions
:mentions rdf:type owl:ObjectProperty ;
          rdfs:domain :Redaction ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Place
                                     <http://www.europeana.eu/schemas/edm/Event>
                                     <http://xmlns.com/foaf/0.1/Organization>
                                     <http://xmlns.com/foaf/0.1/Person>
                                   )
                     ] ;
          rdfs:comment "This property points to an instance of Event  or Person or Organisation or Place that is referred in a given Redaction"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "mentions"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#poeticType
:poeticType rdf:type owl:ObjectProperty ;
            rdfs:domain :PoeticWork ;
            rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                         owl:hasValue <http://postdata.linhd.uned.es/kos/poetic-type>
                       ] ;
            rdfs:comment "Major literary genre of a Poetic Work, that is, drama or poetry. The values of this property are defined in a controlled vocabulary about poetic types"@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "poetic Type"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#realises
:realises rdf:type owl:ObjectProperty ;
          rdfs:domain :Redaction ;
          rdfs:range :PoeticWork ;
          rdfs:comment "This property points to the work (an instance of Poetic Work) of which the text at hand (the current Redaction) is a materialisation"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "realises"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#religiousAffiliation
:religiousAffiliation rdf:type owl:ObjectProperty ;
                      rdfs:domain :Person ;
                      rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                   owl:hasValue <http://postdata.linhd.uned.es/kos/religious-affiliation>
                                 ] ;
                      rdfs:comment "Association of a person with a religion, denomination or sub–denominational religious group. The values of this property are defined in a controlled vocabulary about religious affiliation"@en ;
                      rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                      rdfs:label "religious Affiliation"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#retrievesText
:retrievesText rdf:type owl:ObjectProperty ;
               rdfs:domain :Redaction ;
               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-transmission#Location> ;
               rdfs:comment "This property points to the specific location in a bibliographic item (an instance of Location) which contains the edition used as the base of text at hand"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
               rdfs:label "retrieves Text"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#roleFunction
:roleFunction rdf:type owl:ObjectProperty ;
              rdfs:domain :Role ;
              rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                           owl:hasValue <http://postdata.linhd.uned.es/kos/role-function>
                         ] ;
              rdfs:comment "Specific function performed by some agent on a work (e.g. sponsor, actor, collector, singer, arranger, transcriptor). This is connected to the Role class."@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "role Function"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#school
:school rdf:type owl:ObjectProperty ;
        rdfs:domain :Person ;
        rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                   [ rdf:type owl:Restriction ;
                     owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                     owl:hasValue <http://postdata.linhd.uned.es/kos/school>
                   ] ;
        rdfs:comment "Literary movement/school of a Person"@en ;
        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
        rdfs:label "school"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#socialStatus
:socialStatus rdf:type owl:ObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                           owl:hasValue <http://postdata.linhd.uned.es/kos/social-status>
                         ] ;
              rdfs:comment "The social status of the Person, that is the position or rank of a Person within the society. The values are specified in a controlled vocabulary"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
              rdfs:label "social Status"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#typeOfCharacter
:typeOfCharacter rdf:type owl:ObjectProperty ;
                 rdfs:domain :Role ;
                 rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                              owl:hasValue <http://postdata.linhd.uned.es/kos/character-type>
                            ] ;
                 rdfs:comment "Type of character (eg. “main”, “antagonist”, etc.). The values are specified in a controlled vocabulary"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                 rdfs:label "type Of Character"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#typeOfDesignation
:typeOfDesignation rdf:type owl:ObjectProperty ;
                   rdfs:domain :CreatorRole ;
                   rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                owl:hasValue <http://postdata.linhd.uned.es/kos/designation-type>
                              ] ;
                   rdfs:comment "Type of designation/name. E.g. pseudonym, heteronym, allonym, orthonym variant"@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                   rdfs:label "type Of Designation"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#typeOfTextualElement
:typeOfTextualElement rdf:type owl:ObjectProperty ;
                      rdfs:domain :Redaction ;
                      rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                   owl:hasValue <http://postdata.linhd.uned.es/kos/textual-element-type>
                                 ] ;
                      rdfs:comment "The type of textual element (e.g. part of a greater ensemble; part of a composed collection of poems; prologue of a work; epilogue of a work; a chapter of a work...)"@en ;
                      rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                      rdfs:label "type Of Textual Element"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
<http://www.w3.org/2004/02/skos/core#inScheme> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://postdata.linhd.uned.es/ontology/postdata-core#additionalFile
:additionalFile rdf:type owl:DatatypeProperty ;
                rdfs:domain :Redaction ;
                rdfs:range xsd:anyURI ;
                rdfs:comment """A file (XML, PFD, plain text) which contains the Redaction and, likely, other type of analytical information.

Taking as example the poem \"Amadís de Gaula a Don Quijote de la Mancha\": Redaction could be an XML-TEI file (for example, the TEI annotated text of ADSO corpus (https://github.com/bncolorado/CorpusSonetosSigloDeOro/blob/master/Cervantes/Cervantes_1.xml)), whereas \"additionalFile\" could be another file containing a plain text version of the same poem."""@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "additional File"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#additionalName
:additionalName rdf:type owl:DatatypeProperty ;
                rdfs:domain :Person ;
                rdfs:range xsd:string ;
                rdfs:comment """An additional name, such as a nickname, epithet, or alias, or any other descriptive phrase used within a personal name.

Taking as example the writer Charlotte Brontë, an \"additionalName\" would be her alias: \"Currell Bell\"."""@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "additional Name"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#address
:address rdf:type owl:DatatypeProperty ;
         rdfs:domain :Organisation ;
         rdfs:range xsd:string ;
         rdfs:comment """Contains a postal address, for example of a publisher, an organisation, or an individual. There can only be one address per Organization.

Taking as example Cervantes' poem \"Amadís de Gaula a Don Quijote de la Mancha\", TEI-annotated in the ADSO corpus, the publisher would be \"Natural Language Processing Group. Department of Software and Computing Systems. University of Alicante (Spain)\", and its address would be \"carretera San Vicente s/n 03690 San Vicente del Raspeig, Alicante (Spain)\"."""@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
         rdfs:label "address"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#alternativeName
:alternativeName rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Person ;
                 rdfs:range xsd:string ;
                 rdfs:comment """Alternative name that references the current Person. A person may have zero or several alternative names.

Taking as example the character \"Aldonza Lorenzo\" from the Quixote, an \"alternativeName\" could be \"Dulcinea\"."""@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                 rdfs:label "alternative Name"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#alternativeTitle
:alternativeTitle rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :PoeticWork
                                              :Redaction
                                            )
                              ] ;
                  rdfs:range xsd:string ;
                  rdfs:comment """Alternative title, that is, other known denominations of a Poetic Work or a Redaction.

Taking as example the \"Poema de Mio Cid\", an \"alternativeTitle\" would be \"Cantar de Mio Cid\"."""@en ;
                  rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                  rdfs:label "alternative Title"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#audience
:audience rdf:type owl:DatatypeProperty ;
          rdfs:domain :Redaction ;
          rdfs:range xsd:string ;
          rdfs:comment """The meant readership, viewers or listeners of a cultural product (Redaction). There is at most one possible value for audience.

Taking as example the play \"La segunda Celestina\", \"audience\" would be the Spanish Queen and the people in the court attending the Queen's birthday celebrations."""@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "audience"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#audienceEducationLevel
:audienceEducationLevel rdf:type owl:DatatypeProperty ;
                        rdfs:domain :Redaction ;
                        rdfs:range xsd:string ;
                        rdfs:comment """Level of education of the recipients of a text/song.

Taking as example the play \"La segunda Celestina\" (Agustín de Salazar), \"audienceEducationLevel\" would be a learned audience."""@en ;
                        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                        rdfs:label "audience Education Level"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#biography
:biography rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf <http://purl.org/vocab/bio/0.1/biography> ;
           rdfs:domain :Person ;
           rdfs:range xsd:string ;
           rdfs:comment """An account of a person's life written, composed, or produced by another.

Taking as example the poet Antonio Machado, \"biography\" could be: \"Antonio Cipriano José María y Francisco de Santa Ana Machado y Ruiz (26 July 1875 – 22 February 1939), known as Antonio Machado, was a Spanish poet and one of the leading figures of the Spanish literary movement known as the Generation of '98. His work, initially modernist, evolved towards an intimate form of symbolism with romantic traits. He gradually developed a style characterised by both an engagement with humanity on one side and an almost Taoist contemplation of existence on the other, a synthesis that according to Machado echoed the most ancient popular wisdom. In Gerardo Diego's words, Machado \"spoke in verse and lived in poetry.\"\" (example extracted from Wikipedia)."""@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "biography"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#commentary
:commentary rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :PoeticWork
                                        :Redaction
                                      )
                        ] ;
            rdfs:range xsd:string ;
            rdfs:comment """An explanatory series of notes or comments; an explanatory essay or treatise on a text.

Taking as Opus example \"Versos\" by Fernando de Herrera, the comments Francisco de Quevedo wrote on his own edition along the text of the poems would form the \"commentary\"."""@en ;
            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
            rdfs:label "commentary"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#ethnicity
:ethnicity rdf:type owl:DatatypeProperty ;
           rdfs:domain :Person ;
           rdfs:range xsd:string ;
           rdfs:comment """An ethnic group; a social group that shares a common and distinctive culture, religion, language, or the like.

Taking as Person example the poet \"Antonio Enríquez Gómez\", his \"ethnicity\" would be \"crypto-Jewish\"."""@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
           rdfs:label "ethnicity" .


###  http://postdata.linhd.uned.es/ontology/postdata-core#eventNarration
:eventNarration rdf:type owl:DatatypeProperty ;
                rdfs:domain :Event ;
                rdfs:range xsd:string ;
                rdfs:comment """Text that narrates an Event, that is, an excerpt of the Redaction. There is at most one possible description for this property.

Taking as Opus example the \"Poema de Mio Cid\", and as Event example the Cid's arrival to Burgos, we find an \"eventNarration\" in  lines 1-64, starting with \"De los sos ojos tan fuertemientre llorando\"."""@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
                rdfs:label "event Narration"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#explicit
:explicit rdf:type owl:DatatypeProperty ;
          rdfs:domain :Redaction ;
          rdfs:range xsd:string ;
          rdfs:comment """The closing words of the composition (usually, the last verse). There is only one explicit per Redaction.

Taking as example the poem \"Canary\" by Rita Dove, the last verse, \"If you can't be free, be a mystery\", would be the \"explicit\"."""@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "explicit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#forename
:forename rdf:type owl:DatatypeProperty ;
          rdfs:domain :Person ;
          rdfs:range xsd:string ;
          rdfs:comment """It contains a forename, given or baptismal name.

Taking as example the poet \"Antonio Machado\", his \"forename\" would be \"Antonio\"."""@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "forename"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#function
:function rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :PoeticWork
                                      :Redaction
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment """Purpose of the work (e.g. prayer, spell, curse). A Poetic Work or Redaction may have several functions associated.

Taking as example the poem \"Cantico delle creature\" by San Francesco d’Assisi, \"function\" would be \"prayer\"."""@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
          rdfs:label "function"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#genName
:genName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Person ;
         rdfs:range xsd:string ;
         rdfs:comment """It contains a name component used to distinguish otherwise similar names on the basis of the relative ages or generations of the persons named.

Taking as example the person \"Felipe II\", \"genName\" is \"II\"."""@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
         rdfs:label "genName"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#incipit
:incipit rdf:type owl:DatatypeProperty ;
         rdfs:domain :Redaction ;
         rdfs:range xsd:string ;
         rdfs:comment """The first line of the work. There is one incipit for each Redaction.

Taking as example the \"Soledades\" by Góngora, the \"incipit\" is \"Pasos de un peregrino son errante\"."""@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
         rdfs:label "incipit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isAnonymous
:isAnonymous rdf:type owl:DatatypeProperty ;
             rdfs:domain :CreatorRole ;
             rdfs:range xsd:boolean ;
             rdfs:comment """In case the identity of current instance of Creator is unknown.

Taking as example the creator of \"Libro de Alexandre\", \"isAnonymous\" is True because the author is unknown to this day."""@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "is Anonymous"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isCalligram
:isCalligram rdf:type owl:DatatypeProperty ;
             rdfs:domain :PoeticWork ;
             rdfs:range xsd:boolean ;
             rdfs:comment """Whether it is a calligram, that is, a text visually arranged in a way that it forms an image associated with the text contents.

Taking as example the (calligram) poem \"La Colombe poignardée et le Jet d’eau\" by Apollinaire, \"is Calligram\" value is True."""@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "is Calligram"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isFictional
:isFictional rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Person ;
             rdfs:range xsd:boolean ;
             rdfs:comment """Whether the person under analysis is a fictional character.

Taking as example the character \"Don Quijote de la Mancha\" will have a True value for \"is Fictional\" property."""@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
             rdfs:label "is Fictional"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isSong
:isSong rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :PoeticWork
                                    :Redaction
                                  )
                    ] ;
        rdfs:range xsd:boolean ;
        rdfs:comment """Whether the Redaction was composed to be sung.

Taking as example the poetic work \"Chanson de Roland\", \"is Song\" is True because the work is part of a genre which was sang."""@en ;
        rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
        rdfs:label "is Song"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#isWrongAttribution
