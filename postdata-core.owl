            <owl:Restriction>
                <owl:onProperty rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isReferencedIn"/>
                <owl:allValuesFrom rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-transmission#Location"/>
