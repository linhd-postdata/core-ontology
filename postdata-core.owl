    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#PoeticWork"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/author-education-level"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Level of education of the creator of a text/song. This property has as Domain PoeticWork and not Person since the education level of a person changes during time. That is, this is the education level of the author in the moment of writing the poem.</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">author Education Level</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthDate -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity"/>
        <rdfs:comment xml:lang="en">Date of birth</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birthDate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthPlace -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlace">
        <rdfs:subPropertyOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isFrom"/>
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceOf"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Place"/>
        <rdfs:comment xml:lang="en">This property points to the Place which is the provenance of the current instance of Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birth Place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceCertainty -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceCertainty">
        <rdfs:subPropertyOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#certainty"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:comment xml:lang="en">Certainty of the date of birth of a Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birth Place Certainty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceOf -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceOf">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Place"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:comment xml:lang="en">This property points to a Person who was born in the Place</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birth Place Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#certainty -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#certainty">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/certainty-type"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Level of confidence regarding the correspondent function</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">certainty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#characterIn -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#characterIn">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Agent"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to the text in which the current instance of Person or Organisation is a character.</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">character In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#date -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#date">
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity"/>
        <rdfs:comment xml:lang="en">This property points to a date entity, which represents a particular point or period of time at which something happened or existed, or is expected to happen</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">date</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#deathDate -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#deathDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity"/>
        <rdfs:comment xml:lang="en">Date of death</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">deathDate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#deathPlace -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#deathPlace">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#deathPlaceOf"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Place"/>
        <rdfs:comment xml:lang="en">This property points to the instance of Place where the Person died</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">death Place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#deathPlaceOf -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#deathPlaceOf">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Place"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:comment xml:lang="en">This property points to a Person who died in the current instance of Place</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">death Place Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#dedicatedTo -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#dedicatedTo">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:comment xml:lang="en">This property points to a Person to whom the current text was dedicated. This means that a Redaction includes a dedicated text to another person in an explicit way (frequently at the beginning of the text).</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">dedicated To</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#depictedIn -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#depictedIn">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Role"/>
        <rdfs:comment xml:lang="en">This property points to the work (instance of PoeticWork) or a redaction which depicts the Role at hand in relation to its contents. When the Role at hand is related to the agents that participated in the transmission of the work (e.g. sponsor), this property points to the entity affected by the current function</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">depicted In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#editedIn -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#editedIn">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-transmission#Location"/>
        <rdfs:comment xml:lang="en">This property points to the specific Location in a bibliographic item that contains an edition of the current instance of Redaction.</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">edited In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#editorOf -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#editorOf">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#hasEditor"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to an instance of Redaction which was edited by the the current Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">editor Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#firstPartOf -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#firstPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#hasFirstPart"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#TextUnit"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to the container (an instance of Redaction) of which the item at hand is its first part</rdfs:comment>
        <rdfs:comment xml:lang="en">first Part of</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#floruit -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#floruit">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-dates#DateEntity"/>
        <rdfs:comment xml:lang="en">The period of activity of a person, used when a range cannot be established (e.g. “3rd century”)</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">floruit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#gender -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#gender">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/gender"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The gender of a Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">gender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#genre -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#genre">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#PoeticWork"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/genre"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Type of art, literature or music characterized by a specific form, content and style</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">genre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Agent"/>
        <rdfs:comment xml:lang="en">This property points to a character depicted in the text, if any. The character is an instance of Agent (Organisation or Person).</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has Character</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasCreator -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasCreator">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#Ensemble"/>
                    <rdf:Description rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#PoeticWork"/>
                    <rdf:Description rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#CreatorRole"/>
        <rdfs:comment xml:lang="en">This property points to the entity Creator that has created the current work under analysis</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has Creator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication">
